<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <display-name>SIGRES</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
    <env-entry>
        <env-entry-name>userName</env-entry-name>
        <!-- nao mexer -->
        <env-entry-type>java.lang.String</env-entry-type>
        <!-- nao mexer -->
        <env-entry-value>system</env-entry-value>
        <!-- nome de utilizador  -->
    </env-entry>
    <env-entry>
        <env-entry-name>password</env-entry-name>
        <!-- nao mexer -->
        <env-entry-type>java.lang.String</env-entry-type>
        <!-- nao mexer -->
        <env-entry-value>123</env-entry-value>
        <!-- driver -->
    </env-entry>
    <env-entry>
        <env-entry-name>connectionString</env-entry-name>
        <!-- nao mexer -->
        <env-entry-type>java.lang.String</env-entry-type>
        <!-- nao mexer -->
        <env-entry-value>jdbc:oracle:thin:@localhost:1521:xe</env-entry-value>
        <!-- driver:@servidor:porto:bd-->
    </env-entry>
    <env-entry>
        <env-entry-name>theClass</env-entry-name>
        <!-- nao mexer -->
        <env-entry-type>java.lang.String</env-entry-type>
        <!-- nao mexer -->
        <env-entry-value>oracle.jdbc.driver.OracleDriver</env-entry-value>
        <!-- classe do driver -->
    </env-entry>
</web-app>
